---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title type="text" xml:lang="en">{{ site.name }}</title>
  <link type="application/atom+xml" href="http://davidpaulsson.se/feed.xml" rel="self"/>
  <link type="text" href="http://davidpaulsson.se" rel="alternate"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://davidpaulsson.se</id>
  <author>
    <name>David Paulsson</name>
  </author>
  {% for post in site.posts limit:20 %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://davidpaulsson.se{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://davidpaulsson.se{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}{% include rss-footer.html %}</content>
  </entry>
  {% endfor %}
</feed>
